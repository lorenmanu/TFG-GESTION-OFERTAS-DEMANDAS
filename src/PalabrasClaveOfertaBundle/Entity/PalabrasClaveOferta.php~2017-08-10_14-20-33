<?php

namespace PalabrasClaveOfertaBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * PalabrasClaveOferta
 *
 * @ORM\Table(name="palabras_clave_oferta")
 * @ORM\Entity(repositoryClass="PalabrasClaveOfertaBundle\Repository\PalabrasClaveOfertaRepository")
 */
class PalabrasClaveOferta
{
    /**
     * @var int
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;


    /**
     * Get id
     *
     * @return int
     */
    public function getId()
    {
        return $this->id;
    }
}

